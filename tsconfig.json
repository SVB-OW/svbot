{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		// "esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"outDir": "./out",
		"forceConsistentCasingInFileNames": true,
		"types": ["@types/node"],
		"skipLibCheck": true
	},
	"include": ["./src", "deploy.mjs"]
}
