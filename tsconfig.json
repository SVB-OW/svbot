{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "./out/",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@types/node"],
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "exclude": ["node_modules"]
}
